package _8_serialAndDeserialzation;

public class PayloadData {
	private String name;
	private String job;
	private String Lname; 
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getJob() {
		return job;
	}
	public void setJob(String job) {
		this.job = job;
	}
	
}
